<div class="uk-text-center" style="align-items: center; margin-top: 16px; margin-bottom: 16px;">
    <img class="profileImg" src="{{user.profileImage}}">
</div>

<div id="">
    <h3 class="uk-text-center">Recently Played</h3>
    {{#each recentlyPlayed}}
        <div class="uk-margin-small-left uk-margin-small-bottom">
            <img class="smallPreviewImg uk-inline" src="{{first.album.images.0.url}}">
            <div class="uk-inline uk-margin-small-left" style="color:black;">
                <p style="font-weight: 500; font-size: 20px; margin-bottom: -5px;">{{first.name}}</p>
                <span>{{second}}</span>
            </div>
        </div>
    {{/each}}
</div>